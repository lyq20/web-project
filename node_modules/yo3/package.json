{
  "name": "yo3",
  "version": "3.1.8",
  "description": "一个专注于移动开发的前端框架",
  "compiler": "node-sass",
  "peerDependencies": {
    "react": ">=16.2.0 <16.4.0",
    "react-dom": ">=16.2.0 <16.4.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.9.1",
    "classnames": "^2.2.5",
    "copy-dir": "^0.3.0",
    "lodash": "^4.16.4",
    "normalize-path": "^2.0.1",
    "prop-types": "^15.6.0",
    "raf": "^3.4.0",
    "react-addons-shallow-compare": "^15.3.1",
    "react-tap-event-plugin": "^3.0.2"
  },
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-eslint": "^7.0.0",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "^0.23.1",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "eslint": "^2.9.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.8.0",
    "eslint-plugin-jsx-a11y": "^1.2.0",
    "eslint-plugin-react": "^5.2.2",
    "ghooks": "^1.3.2",
    "immutable": "^3.8.1",
    "jest": "^21.2.1",
    "node-sass": "^4.7.2",
    "qreact": "^2.1.0",
    "react": ">=16.2.0 <16.4.0",
    "react-dom": ">=16.2.0 <16.4.0",
    "rmdir-recursive": "0.0.1",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "validate-commit-msg": "^2.12.2",
    "webpack": "^1.13.1"
  },
  "scripts": {
    "start": "cd .. && sudo ykit server",
    "lint": "ykit lint -d ./component",
    "pack": "node ./builder/pack.js",
    "postinstall": "node builder/postinstall",
    "prepublish": "jest && node builder",
    "test": "jest"
  },
  "repository": "https://github.com/doyoe/Yo",
  "license": "MIT",
  "config": {
    "ghooks": {
      "pre-commit": "jest",
      "commit-msg": "validate-commit-msg"
    },
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "test",
        "chore",
        "refactor",
        "opti"
      ],
      "warnOnFail": false,
      "maxSubjectLength": 100,
      "subjectPattern": ".+",
      "subjectPatternErrorMsg": "请输入message信息!",
      "helpMessage": "Commit message 格式错误, \n请查看规范: http://wiki.corp.qunar.com/pages/viewpage.action?pageId=159698767"
    }
  },
  "jest": {
    "verbose": true,
    "testRegex": "(/__tests__/.*\\.(test|spec))\\.jsx?$",
    "setupTestFrameworkScriptFile": "./jest.setup.js"
  }
}